<h1>Create New Portfolio</h1>

<%= form_for(@portfolio_item) do |f|%>
    <div class="field">
        <%= f.label :title%>
        <%= f.text_field :title%>
    </div>
    <div class="field">
        <%= f.label :subtitle%>
        <%= f.text_field :subtitle%>
    </div>
    <div class="field">
        <%= f.label :body%>
        <%= f.text_field :body%>
    </div>
    <div class="actions">
        <%= f.submit%>
    </div>
    <%= link_to "Back", portfolios_path%>
<% end%>

